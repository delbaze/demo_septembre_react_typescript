{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/interfaces": [
        "./src/interfaces/index.ts"
      ]
      // "@/interfaces": [
      //   "./src/interfaces/CardWilder.d.ts",
      //   "./src/interfaces/Language.d.ts",
      //   "./src/interfaces/Note.d.ts"
      // ]
    }
  },
  "include": [
    "src"
  ],
}